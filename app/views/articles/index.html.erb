<h1>My Articles</h1>
<ul>
<% @articles.each do |article| %>
<li>
<h4> <%= link_to article.title, article_path(article) %></h4>
 <%= link_to 'Update', edit_article_path(article) %>
 <%= button_to "Delete", article_path(article), method: :delete, data: { 'turbo-confirm': "Are you sure?" } %>
</li>
<% end %>
</ul>

 <%= link_to 'Add New Article', new_article_path %>
